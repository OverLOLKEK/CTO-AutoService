<Page x:Class="AutoService.ViewPage.CashBox.TabsCashbox"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AutoService.ViewPage.CashBox"
      mc:Ignorable="d" 
      d:DesignHeight="35" d:DesignWidth="633"
      Title="TabsCashbox">
    <!--Вкладки для кассы-->
    <Grid>
        <GroupBox BorderBrush="{x:Null}" Foreground="{x:Null}">
            <StackPanel Orientation="Horizontal">
                <Button Content="Приход Касса" Width="200" Height="35" Command="{Binding ComingCash}" Background="White"/>
                <Button Content="Расход Касса" Width="200" Height="35" Command="{Binding ConsumptionCash}" Background="White"/>
            </StackPanel>
        </GroupBox>
    </Grid>
</Page>
